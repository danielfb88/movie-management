{
  "name": "ioasys-test-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev -r dotenv/config -P tsconfig.json src/server.ts",
    "migration:create": "typeorm migration:create --dir src/migrations --name",
    "migration:generate": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:generate --name",
    "migration:revert": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:revert",
    "migration:run": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:run",
    "postgres:start": "docker run -d -p 5430:5432 --name postgres -e POSTGRES_HOST_AUTH_METHOD=trust postgres:11-alpine",
    "database:create": "docker exec postgres psql -U postgres postgres -c \"CREATE DATABASE $(grep DB_DATABASE .env | cut -d= -f 2)\"",
    "postgres:down": "docker stop postgres && docker rm postgres",
    "test": "jest --setupFiles dotenv/config -w9 --detectOpenHandles",
    "typecheck": "npx tsc -noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/danielfb88/teste-backend.git"
  },
  "keywords": [
    "challenge",
    "IMDb"
  ],
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/danielfb88/teste-backend#readme",
  "dependencies": {
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "express-validator": "^6.7.0",
    "faker": "^5.1.0",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "source-map-support": "^0.5.19",
    "supertest": "^6.0.1",
    "typeorm": "^0.2.29",
    "typeorm-naming-strategies": "^2.0.0",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.33",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/faker": "^5.1.4",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "git-commit-msg-linter": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.2",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0"
  }
}
